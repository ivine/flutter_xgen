{
  "name": "flutter-xgen",
  "displayName": "Flutter XGen",
  "description": "A plugin designed to create a Flutter resources file from pubspec.yaml.",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Dengwww",
  "author": {
    "name": "Dengwww",
    "email": "vinzry@163.com"
  },
  "icon": "images/icon.png",
  "keywords": [
    "flutter",
    "xgen",
    "assets generate",
    "assets",
    "generate",
    "vscode",
    "watch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ivine/flutter_xgen.git"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FXG.FlutterXGenAssetsGenerate",
        "title": "Flutter XGen: Assets.dart 生成"
      },
      {
        "command": "FXG.FlutterXGenAssetsStartWatch",
        "title": "Flutter XGen: 开始监听 Assets 文件夹"
      },
      {
        "command": "FXG.FlutterXGenAssetsStopWatch",
        "title": "Flutter XGen: 停止监听 Assets 文件夹"
      },
      {
        "command": "FXG.PreviewFile",
        "title": "Flutter XGen: 预览文件"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "FXG",
          "title": "FlutterXGen",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "FXG": [
        {
          "id": "FXG-Assets",
          "name": "Assets"
        },
        {
          "id": "FXG-L10n",
          "name": "l10n"
        }
      ]
    }
  },
  "scripts": {
    "install_all": "yarn && cd webview_ui && yarn",
    "dev_webview": "cd webview_ui && yarn dev",
    "build_webview": "cd webview_ui && yarn build",
    "vscode_prepublish": "npm run compile",
    "compile": "cd webview_ui && yarn build && cd .. && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.12.7",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^9.1.1",
    "glob": "^10.3.12",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vscode-test": "^1.6.1"
  }
}