{
  "name": "flutter-xgen",
  "displayName": "Flutter XGen",
  "description": "A plugin designed to create a Flutter resources file from pubspec.yaml.",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Dengwww",
  "author": {
    "name": "Dengwww",
    "email": "vinzry@163.com"
  },
  "icon": "images/icon.png",
  "keywords": [
    "flutter",
    "xgen",
    "assets generate",
    "assets",
    "generate",
    "vscode",
    "watch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ivine/flutter_xgen.git"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FXG.FlutterXGenAssetsGenerate",
        "title": "Flutter XGen: Assets.dart 生成"
      },
      {
        "command": "FXG.FlutterXGenAssetsStartWatch",
        "title": "Flutter XGen: 开始监听 Assets 文件夹"
      },
      {
        "command": "FXG.FlutterXGenAssetsStopWatch",
        "title": "Flutter XGen: 停止监听 Assets 文件夹"
      },
      {
        "command": "FXG.PreviewFile",
        "title": "Flutter XGen: 预览文件"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "FXG",
          "title": "FlutterXGen",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "FXG": [
        {
          "id": "FXG-Assets",
          "name": "Assets"
        },
        {
          "id": "FXG-L10n",
          "name": "l10n"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.87.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
