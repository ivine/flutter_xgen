{
  "name": "flutter-xgen",
  "displayName": "Flutter XGen",
  "description": "A plugin designed to create a Flutter resources file from pubspec.yaml.",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Dengwww",
  "author": {
    "name": "Dengwww",
    "email": "vinzry@163.com"
  },
  "icon": "images/icon.png",
  "keywords": [
    "flutter",
    "xgen",
    "assets generate",
    "assets",
    "generate",
    "vscode",
    "watch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ivine/flutter_xgen.git"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FXG.FlutterXGenAssetsGenerate",
        "title": "Flutter XGen: Assets.dart 生成"
      },
      {
        "command": "FXG.FlutterXGenAssetsStartWatch",
        "title": "Flutter XGen: 开始监听 Assets 文件夹"
      },
      {
        "command": "FXG.FlutterXGenAssetsStopWatch",
        "title": "Flutter XGen: 停止监听 Assets 文件夹"
      },
      {
        "command": "FXG.PreviewFile",
        "title": "Flutter XGen: 预览文件"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "FXG",
          "title": "FlutterXGen",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "FXG": [
        {
          "id": "FXG-Assets",
          "name": "Assets"
        },
        {
          "id": "FXG-L10n",
          "name": "l10n"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "global": "^4.4.0",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^10.3.12",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
